function() {
  
  function getCookieValue(name) {
    var value = "; " + document.cookie;
    var parts = value.split("; " + name + "=");
    if (parts.length === 2) return parts.pop().split(";").shift();
  }

  
  var leadValue = sessionStorage.getItem('index');

  
  if (!leadValue) {
    leadValue = localStorage.getItem('index');
  }

  
  if (!leadValue) {
    leadValue = getCookieValue('index');
  }

  
  return leadValue || undefined;
}
